{
  "name": "rsc-fun-in-bun",
  "module": "rsc-fun-in-bun.ts",
  "type": "module",
  "scripts": {
    "bunp": "bun add $(bun --print 'Bun.file(`./package.json`).json().then(p => ({...p.devDependencies,...p.dependencies})).then(Object.entries).then(it => it.filter(([,version]) => version.includes(`canary`)).map(([name]) => `${name}@canary`).join(` `))'); bun update",
    "predev": "bun bunp",
    "react-server": "bun --config=react-server.bunfig.toml --conditions=react-server",
    "test:ui": "PUPPETEER_HEADLESS=true bun test",
    "test:ui:open": "PUPPETEER_HEADLESS=false bun test",
    "dev": "bun run react-server --inspect --watch rsc-fun-in-bun.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/use-sync-external-store": "^0.0.6",
    "bun-plugin-server-components": "^0.0.1-alpha.0",
    "prettier": "latest",
    "puppeteer-core": "^22.6.1",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "peechy": "^0.4.34",
    "react": "^19.0.0-canary-e3ebcd54b-20240405",
    "react-dom": "^19.0.0-canary-e3ebcd54b-20240405",
    "react-server-dom-webpack": "^19.0.0-canary-e3ebcd54b-20240405",
    "use-sync-external-store": "^1.4.0-canary-e3ebcd54b-20240405"
  }
}
